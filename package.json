{
  "name": "tykbasic",
  "version": "1.0.0",
  "description": "TykBasic - A streamlined frontend application for managing Tyk Gateway APIs, organizations, users, and authentication methods",
  "main": "backend/server.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cross-env NODE_ENV=development nodemon backend/server.js",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:start": "node backend/server.js",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "setup": "npm install && npm run frontend:install && npm run dev-setup",
    "dev-setup": "node scripts/dev-setup.js",
    "docker:build": "docker build -t tykbasic .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f tykbasic",
    "lint": "eslint backend/ --ext .js",
    "lint:fix": "eslint backend/ --ext .js --fix"
  },
  "keywords": [
    "tyk",
    "api-management",
    "gateway",
    "authentication",
    "dashboard"
  ],
  "author": "TykBasic Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "connect-session-sequelize": "^7.1.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-brute": "^1.0.1",
    "express-brute-sequelize": "^0.0.9",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.10.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.0",
    "sib-api-v3-sdk": "^8.5.0",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
